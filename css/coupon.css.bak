
.ui-content {
	border-width: 0;
	overflow: visible;
	overflow-x: hidden;
	padding: 2px;
}

/*.ui-btn {
	display: block;
	text-align: right;
	cursor: pointer;
	position: relative;
	margin: .5em;
	padding: 0;
	font-size: small;
}*/

#back-button {
	width: 200px;
	text-align: right;
}

#image-block {
	float: left;
	height: 130px;
}

#coupon-img {
	/*float: left;*/
	position: relative;
	left: +5px;
	top: +5px;
	width: 120px;
	height: 100%;
	overflow: hidden;
}

#info {
	float: right;
}

#ul-details {
	list-style-type: none;
	-webkit-margin-before: 1em;
	-webkit-margin-after: 1em;
	-webkit-margin-start: 0px;
	-webkit-margin-end: 0px;
	-webkit-padding-start: 0px;
}

#li-details {
	margin-bottom: 1em;
}

#coupon-details {
	/*float: left;*/
	height: 130px;
	position: relative;
	font-family:"Arial", Arial, serif;
	text-align: justified;
}

#coupon-discount {
	color: red;	
	font-weight: bold;
}

#coupon-date {
	font-style: italic;
	position: relative;
}








